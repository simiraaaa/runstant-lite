panel-project
  div.preview

  style.
    panel-project {
    
    }
    panel-project .preview {
      width: 100%;
      height: 100%;
      -webkit-overflow-scrolling: touch;
    }
    panel-project .preview iframe {
      width: 100%;
      height: 100%;
      border: none;
    }

  script.
    var self = this;

    this.on('mount', function() {
      this.refresh();

      this.on('update', function() {
        this.refresh();
      });
    });

    this.refresh = function() {
      if (!self.jframe) {
        self.jframe = jframe('panel-project .preview');
      }
      var v = runstant.project.toProject();
      self.jframe.load(v);
    };



